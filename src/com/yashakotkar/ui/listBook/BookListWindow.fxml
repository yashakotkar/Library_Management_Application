<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="mainContainer" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.yashakotkar.ui.listBook.BookListWindowController">
    <TableView fx:id="bookListTable" layoutX="14.0" layoutY="14.0" prefHeight="400.0" prefWidth="600.0"
               AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
      <columns>
          <TableColumn fx:id="idCol" prefWidth="75.0" text="ID"/>
          <TableColumn fx:id="titleCol" prefWidth="75.0" text="TITLE"/>
          <TableColumn fx:id="authorCol" prefWidth="75.0" text="AUTHOR"/>
          <TableColumn fx:id="publisherCol" prefWidth="75.0" text="PUBLISHER"/>
          <TableColumn fx:id="quantityCol" prefWidth="75.0" text="QUANTITY"/>
      </columns>
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</AnchorPane>
